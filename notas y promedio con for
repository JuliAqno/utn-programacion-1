#include <stdio.h>

int main() {
	int a,i;
	float b,contador;
	contador=0;
	i=0;
	printf("ingrese la cantidad de notas que desea ingresar: ");
	scanf("%d",&a);
	for(i=1;i<=a;i++){
		printf("calificacion: ");
		scanf("%f",&b);
		contador=contador+b;
	}
	printf("el promedio es de %.2f",contador/a);
	return 0;
}

