#include <stdio.h>
#define max_element 5


int main() {
	int array[max_element]={5,3,2};
	printf("Arreglo original");
	for(int i=0;i<max_element;i++){
		printf("\nelemento de la posicion %d: %d",i,array[i]);
	}
	
	for(int i=0;i<max_element;i++){
		if(i==3){
			array[i]=1;
		}
		
		if(i==4){
			array[i]=10;
			
		}
	}
	printf("\narreglo modificado");
	for(int i=0;i<max_element;i++){
		printf("\nelemento de la posicion %d: %d",i,array[i]);
	}
	
	
	return 0;
}
